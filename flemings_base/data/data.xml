<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">

		<record id="stock_route_buy_external" model="stock.route">
			<field name="name">Buy - External</field>
			<field name="is_buy_external" eval="True"/>
			<field name="is_access_restrict" eval="True"/>
			<field name="company_id" eval="False"/>
			<field name="product_selectable" eval="True"/>
			<field name="sale_selectable" eval="True"/>
		</record>

		<record id="stock_route_buy_internal" model="stock.route">
			<field name="name">Buy - Internal</field>
			<field name="is_buy_internal" eval="True"/>
			<field name="is_access_restrict" eval="True"/>
			<field name="company_id" eval="False"/>
			<field name="product_selectable" eval="True"/>
			<field name="sale_selectable" eval="True"/>
		</record>

		<!-- Sequences -->
		<record id="ir_sequence_mrp_production_work_order_number" model="ir.sequence">
			<field name="name">Manufacturing - Work Order No.</field>
			<field name="code">mrp.production.work.order.no</field>
			<field name="prefix">WO/</field>
			<field name="padding">4</field>
			<field name="number_increment">1</field>
			<field name="number_next_actual">1</field>
			<field name="implementation">standard</field>
			<field name="company_id" eval="False"/>
		</record>

	</data>
</odoo>
